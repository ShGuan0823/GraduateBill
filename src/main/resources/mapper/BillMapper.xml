<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.njupt.ciot.graduatebill.dao.BillDao">
    <select id="getCet4" resultType="java.lang.String" parameterType="java.lang.String">
        SELECT grade FROM CET-4 WHERE stuId = #{stuId}
    </select>
    <select id="getCet6" resultType="java.lang.String" parameterMap="java.lang.String">
        SELECT grade FROM CET-6 WHERE stuId = #{stuId}
    </select>
    <select id="getBookNum" resultType="java.lang.String" parameterType="java.lang.String">
        SELECT borrowNum FROM library WHERE stuId = #{stuId}
    </select>
    <select id="getGrade" resultType="com.njupt.ciot.graduatebill.domain.Grade" parameterType="java.lang.String">
        SELECT * FROM grade WHERE stuId = #{stuId}
    </select>
    <select id="getLevelExam" resultType="com.njupt.ciot.graduatebill.domain.LevelExam" parameterType="java.lang.String">
        SELECT * FROM level_exam WHERE stuId = #{stuId}
    </select>
</mapper>